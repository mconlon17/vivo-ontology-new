robot template \
  --template ado.tsv \
  --ontology-iri "https://something/ontology/ado/" \
  --prefix "ADO: https://something/ontology/ado/ADO_" \
  --version-iri "https://something/ontology/ado/0.0.0/" \
  convert \
  --output temp.ttl
grep -v ^# temp.ttl >ado.ttl
rm temp.ttl
robot merge --input ado-header.owl --input ado.ttl --output ado.owl
robot report --input ado.owl --output ado-report.tsv
